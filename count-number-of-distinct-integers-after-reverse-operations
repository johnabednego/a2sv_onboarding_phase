class Solution:
    def countDistinctIntegers(self, nums: List[int]) -> int:
        dummy = []
        for num in nums:
            string = str(num)
            reversed_num = int(string[::-1])
            dummy.append(reversed_num)
        for num in nums:
            dummy.append(num)
        uniques_set = set()
        for num in dummy:
            uniques_set.add(num)
        return len(uniques_set)
